.expectancy-area(ng-show="currentPage > -1")
  h1 Imagine you lived in a different country... <span>what would be your life expectancy?</span>
  angucomplete(
    placeholder="Search countries",
    pause="100",
    selectedobject="selectedCountry",
    localdata="countries",
    searchfields="name",
    titlefield="name",
    minlength="1",
    inputclass="form-control form-control-small"
  )
  expectancy-map